<?xml version="1.0"?>
<root main_tree_to_execute="RMUC2024">
  <!-- ///// 此树用于2024分区赛 ///// -->
  <BehaviorTree ID="RMUC2024">
    <Fallback>
      <Sequence>

        <Condition ID="gameStart"/>

        <Fallback>

          <!-- 一、云台手要求——强制待在巡逻区 -->
          <Sequence>
            <Condition ID="forceBack"/>
            <Fallback>
              <!-- 1-设置目标 -->
              <Sequence>
                <Fallback>
                  <!-- 敌方前哨站存活——ExceptSentry -->
                  <Sequence>
                    <Condition ID="enemyOutpostSurvives"/>
                    <Action ID="SetRmosTarget" service_name="aim_target"
                      list_name="ExceptSentry" 
                      low_hp_list="{low_hp_list}" 
                      invincibility_list="{invincibility_list}" 
                      gameover_time="{gameover_time}" 
                    />
                  </Sequence>
                  <!-- 所有 -->
                  <Sequence>
                    <Action ID="SetRmosTarget" service_name="aim_target"
                      list_name="All" 
                      low_hp_list="{low_hp_list}" 
                      invincibility_list="{invincibility_list}" 
                      gameover_time="{gameover_time}" 
                    />
                  </Sequence>
                </Fallback>
                <Inverter>
                  <Condition ID="gameStart"/> <!-- TODO : 强制错误 -->
                </Inverter>
              </Sequence>
              <!-- ^^^^^^^^^^^^^^^^^^^ -->
              <!-- 2-在巡逻区 有目标 -->
              <Sequence>
                <Condition ID="inPatrol"/>
                <Condition ID="haveTarget"/>
                <Action ID="SetStaticNavTarget" service_name="nav_target"
                  have_target="{have_target}" 
                  gimbal="{gimbal}" 
                  target_pos="{target_pos}"
                />
              </Sequence>
              <!-- ^^^^^^^^^^^^^^^^^^^ -->
              <!-- 3-不在巡逻区或者无目标 -->
              <Action ID="GoBack" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
              <!-- ^^^^^^^^^^^^^^^^^^^ -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 二、前哨站血量小于300且基地护甲未展开且离开时间大于一定值——必须回巡逻区 -->
          <Sequence>
            <Condition ID="outpostLowLowHp"/>
            <Inverter>
              <Condition ID="baseUnfolds"/>
            </Inverter>
            <Condition ID="leaveTooLong"/>
            <Fallback>
              <Action ID="SetRmosTarget" service_name="aim_target"
                list_name="None" 
                low_hp_list="{low_hp_list}" 
                invincibility_list="{invincibility_list}" 
                gameover_time="{gameover_time}" 
              />
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
            <Fallback>
              <Action ID="GoBack" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 三、比赛时间剩余20秒且基地护甲未展开且以哨兵血量判负——回补给区 -->
          <Sequence>
            <Condition ID="gameAboutOver"/>
            <Inverter>
              <Condition ID="baseUnfolds"/>
            </Inverter>
            <Condition ID="sentryFailed"/>
            <Fallback>
              <Action ID="RmucGoSupply" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 四、如果在补给区并且可以补给弹丸——补给弹丸 -->
          <Sequence>
            <Condition ID="inSupply"/>
            <Condition ID="canBuyBullets"/>
            <Fallback>
              <Action ID="SetRmosTarget" service_name="aim_target"
                list_name="None" 
                low_hp_list="{low_hp_list}" 
                invincibility_list="{invincibility_list}" 
                gameover_time="{gameover_time}" 
              />
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
            <Action ID="BuyBullets" service_name="buy_bullets"
              buy_bullets="{buy_bullets}" 
            />
          </Sequence>
          <!-- ///////////////// -->

          <!-- 五、如果在补给区且血量不充足——回补给区 -->
          <Sequence>
            <Condition ID="inSupply"/>
            <Inverter>
              <Condition ID="enoughHp"/>
            </Inverter>
            <Fallback>
              <Action ID="SetRmosTarget" service_name="aim_target"
                list_name="None" 
                low_hp_list="{low_hp_list}" 
                invincibility_list="{invincibility_list}" 
                gameover_time="{gameover_time}" 
              />
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
            <Fallback>
              <Action ID="RmucGoSupply" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 六、如果需要解锁发射机构——回补给区 -->
          <Sequence>
            <Condition ID="needUnlock"/>
            <Fallback>
              <Action ID="SetRmosTarget" service_name="aim_target"
                list_name="None" 
                low_hp_list="{low_hp_list}" 
                invincibility_list="{invincibility_list}" 
                gameover_time="{gameover_time}" 
              />
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
            <Fallback>
              <Action ID="RmucGoSupply" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 七、如果没弹丸且可以补给弹丸且（前哨站存活或基地装甲板展开）——回补给区 -->
          <Sequence>
            <Condition ID="lowBullets"/>
            <Condition ID="canBuyBullets"/>
            <Fallback>
              <Condition ID="outpostSurvives"/>
              <Condition ID="baseUnfolds"/>
            </Fallback>
            <Fallback>
              <Action ID="SetRmosTarget" service_name="aim_target"
                list_name="None" 
                low_hp_list="{low_hp_list}" 
                invincibility_list="{invincibility_list}" 
                gameover_time="{gameover_time}" 
              />
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
            <Fallback>
              <Action ID="RmucGoSupply" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 七、如果前哨站血量少且没有领取过子弹且满血——回补给区 -->
          <Sequence>
            <Condition ID="outpostLowLowHp"/>
            <Inverter>
              <Condition ID="firstGetBullets"/>
            </Inverter>
            <Condition ID="fullHp"/>
            <Fallback>
              <Action ID="RmucGoSupply" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 八、如果血少且基地未展开——回补给区 -->
          <Sequence>
            <Condition ID="lowHp"/>
            <Inverter>
              <Condition ID="baseUnfolds"/>
            </Inverter>
            <Fallback>
              <Action ID="SetRmosTarget" service_name="aim_target"
                list_name="None" 
                low_hp_list="{low_hp_list}" 
                invincibility_list="{invincibility_list}" 
                gameover_time="{gameover_time}" 
              />
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
            <Fallback>
              <Action ID="RmucGoSupply" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 九、如果没弹丸——乱跑 -->
          <Sequence>
            <Condition ID="lowBullets"/>
            <Fallback>
              <Action ID="SetRmosTarget" service_name="aim_target"
                list_name="None" 
                low_hp_list="{low_hp_list}" 
                invincibility_list="{invincibility_list}" 
                gameover_time="{gameover_time}" 
              />
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
            <Fallback>
              <Action ID="RmucPatrol" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 如果需要躲避且基地护甲未展开——一直躲避 -->
          <Sequence>
            <Condition ID="needAvoid"/>
            <Inverter>
              <Condition ID="baseUnfolds"/>
            </Inverter>
            <Fallback>
              <Action ID="RmucPatrol" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 十、如果需要反前哨站且敌方前哨站存活且前哨站血量不更低且前哨站不停转——到台阶上 如果到台阶了且有目标——打 -->
          <Sequence>
            <Condition ID="counterOutpost"/>
            <Condition ID="enemyOutpostSurvives"/>
            <Inverter>
              <Condition ID="outpostLowLowHp"/>
            </Inverter>
            <Inverter>
              <Condition ID="outpostStalled"/>
            </Inverter>
            <Fallback>
              <Sequence>
                <Action ID="RmucGoEnemyOutpost" service_name="nav_goal"/>
                <Fallback>
                  <Sequence>
                    <Action ID="SetRmosTarget" service_name="aim_target"
                      list_name="OnlyOutpost" 
                      low_hp_list="{low_hp_list}" 
                      invincibility_list="{invincibility_list}" 
                      gameover_time="{gameover_time}" 
                    />
                    <Action ID="SetOutpostTarget" service_name="nav_target"/>
                  </Sequence>
                  <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
                </Fallback>
              </Sequence>
              <Fallback>
                <Action ID="SetRmosTarget" service_name="aim_target"
                  list_name="None" 
                  low_hp_list="{low_hp_list}" 
                  invincibility_list="{invincibility_list}" 
                  gameover_time="{gameover_time}" 
                />
                <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
              </Fallback>
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <Sequence>
            <Fallback>
              <!-- 敌方前哨站存活——ExceptSentry -->
              <Sequence>
                <Condition ID="enemyOutpostSurvives"/>
                <Action ID="SetRmosTarget" service_name="aim_target"
                  list_name="ExceptSentry" 
                  low_hp_list="{low_hp_list}" 
                  invincibility_list="{invincibility_list}" 
                  gameover_time="{gameover_time}" 
                />
              </Sequence>
              <!-- 所有 -->
              <Sequence>
                <Action ID="SetRmosTarget" service_name="aim_target"
                  list_name="All" 
                  low_hp_list="{low_hp_list}" 
                  invincibility_list="{invincibility_list}" 
                  gameover_time="{gameover_time}" 
                />
              </Sequence>
            </Fallback>  
            <Inverter>
              <Condition ID="gameStart"/> <!-- TODO : 强制错误 -->
            </Inverter>
          </Sequence>

          <!-- 十一、如果有目标且（(前哨站血量不低或基地展开或安全)-->
          <Sequence>
            <Condition ID="haveTarget"/>
            <Fallback>
              <Inverter>
                <Condition ID="outpostLowHp"/>
              </Inverter>
              <Condition ID="baseUnfolds"/>
              <Action ID="RmucIsSave" service_name="nav_goal"/>
            </Fallback>
            <Fallback>
              <!-- 1-(前哨站不血量更低或基地展开)且可以跟随——动态跟随 -->
              <Sequence>
                <Fallback>
                  <Inverter>
                    <Condition ID="outpostLowLowHp"/>
                  </Inverter>
                  <Condition ID="baseUnfolds"/>
                </Fallback>
                <Action ID="SetNavTarget" service_name="nav_target"
                  have_target="{have_target}" 
                  gimbal="{gimbal}" 
                  target_pos="{target_pos}"
                />
              </Sequence>
              <!-- ^^^^^^^^^^^^^^^^^^^ -->
              <!-- 2-否则静态 -->
              <Action ID="SetStaticNavTarget" service_name="nav_target"
                have_target="{have_target}" 
                gimbal="{gimbal}" 
                target_pos="{target_pos}"
              />
              <!-- ^^^^^^^^^^^^^^^^^^^ -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 十二、如果基地护甲展开——去敌方区域-->
          <Sequence>
            <Condition ID="baseUnfolds"/>
            <Fallback>
              <Action ID="RmucEnemyPatrol" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 十三、如果前哨站血量不低 -->
          <Sequence>
            <Inverter>
              <Condition ID="outpostLowHp"/>
            </Inverter>

            <Fallback>
              <!-- 1-模式二——巡逻区巡逻 最保守 -->
              <Sequence>
                <Condition ID="centerPatrol"/>
                <Fallback>
                  <Action ID="RmucPatrol" service_name="nav_goal"/>
                  <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
                </Fallback>
              </Sequence>
              <!-- ^^^^^^^^^^^^^^^^^^^ -->

              <!-- 2-模式一且前哨站不停转——对面小资源岛巡逻 -->
              <Sequence>
                <Condition ID="enemyAreaPatrol"/>
                <Inverter>
                  <Condition ID="outpostStalled"/>
                </Inverter>
                <Fallback>
                  <Action ID="RmucEnemyAreaPatrol" service_name="nav_goal"/>
                  <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
                </Fallback>
              </Sequence>
              <!-- ^^^^^^^^^^^^^^^^^^^ -->
            </Fallback>

          </Sequence>
          <!-- ///////////////// -->

          <!-- 十四、不是最保守模式且前哨站血量不更低——去前哨站附近 -->
          <Sequence>
            <Inverter>
              <Condition ID="centerPatrol"/>
            </Inverter>
            <Inverter>
              <Condition ID="outpostLowLowHp"/>
            </Inverter>
            <Fallback>
              <Action ID="RmucEnemySlope" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

          <!-- 十五、巡逻 -->
          <Sequence>
            <Fallback>
              <Action ID="RmucPatrol" service_name="nav_goal"/>
              <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
            </Fallback>
          </Sequence>
          <!-- ///////////////// -->

        </Fallback>
        
      </Sequence>

      <Sequence>
        <Condition ID="counterOutpost"/>
        <Fallback>
          <Action ID="RmucGoLeft" service_name="nav_goal"/>
          <Inverter>
            <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
          </Inverter>
        </Fallback>
      </Sequence>

      <Sequence>
        <Condition ID="enemyAreaPatrol"/>
        <Fallback>
          <Action ID="RmucGoRight" service_name="nav_goal"/>
          <Inverter>
            <Condition ID="gameStart"/> <!-- TODO : 强制正确 -->
          </Inverter>
        </Fallback>        
      </Sequence>

      <Action ID="GoBack" service_name="nav_goal"/>
    </Fallback>
  </BehaviorTree>

</root>
