<?xml version="1.0"?>
<root main_tree_to_execute="RMUC2024">
  <!-- ///// 此树用于2024分区赛 ///// -->
  <BehaviorTree ID="RMUC2024">
    <Fallback>
          <Condition ID="needUnlock"/>
          <Sequence>
            <Condition ID="inSupply"/>
            <Condition ID="canBuyBullets"/>
            <Action ID="BuyBullets" service_name="buy_bullets"
              buy_bullets="{buy_bullets}" 
            />
          </Sequence>

    </Fallback>
  </BehaviorTree>

</root>
